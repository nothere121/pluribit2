{
  "name": "pluribit-node",
  "version": "1.0.0",
  "description": "A Node.js implementation of the Pluribit cryptocurrency.",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "build:proto": "pbjs -t static-module -w commonjs -o src/p2p_pb.cjs src/p2p.proto && pbts -o src/p2p_pb.d.ts src/p2p_pb.cjs",
    "build": "npm run build:proto && wasm-pack build --target nodejs -d ./pkg-node && cd native && npm run build && cd ..",
    "start": "node main.js",
    "test": "c8 --all node test.js && cargo test --lib && wasm-pack test --node"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@chainsafe/libp2p-gossipsub": "^14.1.1",
    "@chainsafe/libp2p-noise": "^16.1.4",
    "@chainsafe/libp2p-yamux": "^7.0.4",
    "@libp2p/autonat": "^2.0.37",
    "@libp2p/bootstrap": "^11.0.46",
    "@libp2p/circuit-relay-v2": "^2.1.2",
    "@libp2p/crypto": "^5.1.7",
    "@libp2p/dcutr": "^2.0.37",
    "@libp2p/identify": "^3.0.38",
    "@libp2p/interface-peer-id": "^2.0.2",
    "@libp2p/kad-dht": "^15.1.10",
    "@libp2p/mdns": "^11.0.46",
    "@libp2p/mplex": "^11.0.46",
    "@libp2p/peer-id-factory": "^4.2.4",
    "@libp2p/ping": "^2.0.36",
    "@libp2p/tcp": "^10.1.18",
    "@libp2p/webrtc": "^5.2.23",
    "@libp2p/websockets": "^9.2.18",
    "@multiformats/multiaddr": "^12.1.3",
    "chalk": "^5.3.0",
    "express": "^4.21.2",
    "inquirer": "^9.2.20",
    "it-pipe": "^3.0.1",
    "level": "^8.0.1",
    "libp2p": "^2.9.0",
    "multiformats": "^13.4.0",
    "pipe": "^1.1.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "c8": "^10.1.3",
    "esmock": "^2.7.1",
    "fast-check": "^4.2.0",
    "memdown": "^6.1.1",
    "protobufjs-cli": "^1.1.2",
    "sinon": "^21.0.0",
    "tape": "^5.9.0"
  }
}
